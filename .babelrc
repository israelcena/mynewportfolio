{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }]
  ],
  "overrides": [
    {
      "test": ["./node_modules"],
      "plugins": [["styled-components", { "ssr": false }]]
    }
  ],
  "sourceMaps": true,
  "retainLines": true,
  "compact": true,
  "comments": false
}
